   ],
   "source": [
    "##Modelo Polinomial\n",
    "x_m = x[:]\n",
    "y_m = y[:]\n",
    "# Modelo polinomial\n",
    "grado = len(x)\n",
    "\n",
    "poli = pd.DataFrame(columns=('Días', 'Polinomial')) \n",
    "y_pol = np.poly1d(np.polyfit(x_m,y_m,grado))\n",
    "\n",
    "# Asignación de valores a la variable dependiente según la ecuación anterior\n",
    "for pred in x_m: \n",
    "    poli.loc[len(poli)] = [pred, y_pol(pred)]\n"
   ]
